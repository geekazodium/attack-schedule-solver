windows.debug.x86_64 =   "res://../rust-solver/target/debug/attack_scheduler.dll"
windows.release.x86_64 = "res://../rust-solver/target/release/attack_scheduler.dll"
macos.debug =            "res://../rust-solver/target/debug/libattack_scheduler.dylib"
macos.release =          "res://../rust-solver/target/release/libattack_scheduler.dylib"
macos.debug.arm64 =      "res://../rust-solver/target/debug/libattack_scheduler.dylib"
macos.release.arm64 =    "res://../rust-solver/target/release/libattack_scheduler.dylib"